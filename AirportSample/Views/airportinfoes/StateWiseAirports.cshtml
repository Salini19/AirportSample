@model IEnumerable<AirportSample.Models.State>

<html>
<head>
    <link href="~/Content/StateWiseAirports.css" rel="stylesheet" />
</head>
<body >

    <div class="container-fluid" style="margin-top:10px;">
        <div class="row">
            @*@foreach (Cooking_App.Models.State m in ViewBag.StateList)*@
            @foreach (var m in Model)
            {
                <div class="col-sm-4">
                    <div class="card" style="margin-top:15px;">
                        <div class="card-body">
                           <a href="~/Controllers/airportinfoesController.cs" style="color:black"> <h6 class="card-title">@m.Sname.ToUpper()</h6></a>
                            <p id="line" style="border-top: 1px solid #B2BABB;"></p>
                            <span id="actlink">@Html.ActionLink("Click Here", "AirportList", new { @id = m.Sname })</span>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
    <script src="https://unpkg.com/ionicons@5.4.0/dist/ionicons.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
</body>
</html>
